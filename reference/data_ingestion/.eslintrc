{
  "env": {
    "browser": true,
    "commonjs": true,
    "jest": false,
    "mocha": false,
    "es6": true
  },
  "extends": [
  ],
  "rules": {
    "no-undef": "off",
    "no-redeclare": "error",
    "no-irregular-whitespace": "error",
    "no-trailing-spaces": "error",
    "no-useless-escape": "error",
    "no-class-assign": "error",
    "no-inner-declarations": "error",
    "no-unreachable": "error",
    "no-debugger": "off",
    "no-console": "off",
    "no-unused-vars":"off",
    "prefer-const": "error",
    "eol-last": "error",
    "indent": [
      "error",
      2
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      2,
      "always"
    ]
  }
}